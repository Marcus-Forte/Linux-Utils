# 'Host' can be used in shell to map different users. check below.

# IdentityFile ~/.ssh/id_rsa
# IdentitiesOnly yes

Host github-duna
	Hostname github.com
	User git
	IdentityFile ~/.ssh/id_rsa_duna
	IdentitiesOnly yes

Host github-dee
	Hostname github.com
	User git
	IdentityFile ~/.ssh/id_rsa 
	IdentitiesOnly yes
